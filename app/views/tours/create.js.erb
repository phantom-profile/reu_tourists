<% if @tour.valid? %>
  $('.add-tours-errors').html('')
  $('.tours-table').append('<%=j render partial: 'tour', locals: { tour: @tour }%>')
  $('.form-control').each(function(index) {
      this.value = ""
  })
  $("#tour_tourist_id").children('[value=<%= @tour.tourist.id %>]').remove();
<% else %>
  $('.add-tours-errors').html('<%=j render partial: 'shared/errors_list', locals: { errors: @tour.errors }%>')
<%end%>
