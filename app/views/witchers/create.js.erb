<% if @witcher.valid? %>
  $('.add-witchers-errors').html('')
  $('.witchers-table').append('<%=j render partial: 'witcher', locals: { witcher: @witcher }%>')
  $('.form-control').each(function(index) {
      this.value = ""
  })
  $("#witcher_sword").children('[value=<%= @witcher.sword.id %>]').remove();
<% else %>
  $('.add-witchers-errors').html('<%=j render partial: 'shared/errors_list', locals: { errors: @witcher.errors }%>')
<%end%>
